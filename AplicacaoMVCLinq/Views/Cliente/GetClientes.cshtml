@{
}
@model IEnumerable<AplicacaoMVCLinq.Models.Clientes>
@{
    ViewData["Title"] = "Listar Clientes";
}

<h1> Listar Clientes </h1>
<table>
    <thead>
        <tr>
            <th>Clientesid</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Telefone</th>
            <th>DataNasc</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var clientes in Model)
        {
            <tr>
                <td>@clientes.ClientesId</td>
                <td>@clientes.Nome</td>
                <td>@clientes.Email</td>
                <td>@clientes.Telefone</td>
                <td>@clientes.DataNasc/td>
            </tr>
        }
    </tbody>
</table>
